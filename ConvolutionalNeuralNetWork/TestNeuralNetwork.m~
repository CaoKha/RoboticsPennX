load('X.mat');
load('Y.mat');   
X=StandardizeData(X);
[train_x, train_y, test_x, test_y] = splitData(X, Y);
nn=TrainNetwork(train_x,train_y);
preds=PredictNetwork(nn,test_x);
ground_truth = zeros(857,1);
for i = 1:857
    for j = 1:20
        if test_y(i,j) == 1
            ground_truth(i) = j;
        end
    end
end
cou
for id = 1: 857
    if preds(id) == ground_truth(id)
        count = count +1;